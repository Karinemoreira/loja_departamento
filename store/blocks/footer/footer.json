{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#footer-desktop",
      "newsletter",
      "footer-spacer",
      "social-networks"
    ]
  },
  "newsletter": {
    "props": {
      "blockClass": "news",
      "label": "Quer ficar por dentro de tudo?",
      "placeholder": "Digite seu e-mail",
      "submit": "Cadastrar"
    }
  },
  "social-networks": {
    "props": {
      "showInColor": false,
      "socialNetworks": [
        {
          "name": "Facebook",
          "src": "assets/imgs/facebook.png"
        },
        {
          "name": "Instagram",
          "src": "assets/imgs/instagram.png"
        },
        {
          "name": "Youtube",
          "src": "assets/imgs/youtube.png"
        }
      ]
    }
  },
  "flex-layout.row#footer-2-desktop": {
    "children": [
      "accepted-payment-methods"
    ],
    "props": {
      "blockClass": "payment-methods"
    }
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": [
        "MasterCard",
        "Visa",
        "Diners Club"
      ]
    }
  },
  "flex-layout.row#footer-3-desktop": {
    "children": [],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": [
      "flex-layout.col#footer-1-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "newsletter",
      "vtex.menu@2.x:menu#footer-mobile",
      "social-networks",
      "flex-layout.row#paymentImages",
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.row#paymentImages": {
    "props": {
      "preserveLayoutOnMobile": true,
      "blockClass": "paymentImgs",
      "width": "50%"
    },
    "children": [
      "image#visa",
      "image#master",
      "image#dinners",
      "image#amex",
      "image#elo",
      "image#billet"
    ]
  },
  "image#visa": {
    "props": {
      "blockClass": "visaImg",
      "src": "assets/imgs/visa.png"
    }
  },
  "image#master": {
    "props": {
      "blockClass": "masterImg",
      "src": "assets/imgs/mastercard.png",
      "height": 30,
      "width": 35
    }
  },
  "image#dinners": {
    "props": {
      "blockClass": "dinnersImg",
      "src": "assets/imgs/dinners.png"
    }
  },
  "image#amex": {
    "props": {
      "blockClass": "amexImg",
      "src": "assets/imgs/amex.png",
      "height": 35,
      "width": 35
    }
  },
  "image#elo": {
    "props": {
      "blockClass": "eloImg",
      "src": "assets/imgs/elo.png",
      "height": 30,
      "width": 35
    }
  },
  "image#billet": {
    "props": {
      "blockClass": "billetImg",
      "src": "assets/imgs/billet.png",
      "height": 25,
      "width": 25
    }
  },
  "rich-text#footer-mobile": {
    "props": {
      "textPosition": "center",
      "text": "AVANTI S.A Â© 2020. TODOS OS DIREITOS RESERVADOS.",
      "blockClass": "footer"
    }
  }
}